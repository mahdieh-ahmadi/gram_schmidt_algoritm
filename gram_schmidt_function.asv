function [q , dependents] = gram_schmidt_function(inputMatrix)
sizeMatrix = size(inputMatrix);
counter = sizeMatrix(2);
q=[inputMatrix(:,1)/norm(inputMatrix(:,1))];
dependents = [];

for i=2:1:counter
     ak = inputMatrix(:,i);
     sum = ak;
    for j=1:1:i-1
       sum = sum - ( q(:,j)'*ak).*q(:,j);
    end
    if (norm(sum)<0.1)
        dependents =[dependents , i];
        sum = zeros(sizeMatrix(1),1);
        q =[q, sum];
    else
    q =[q, sum/norm(sum)];
    end
end




end

